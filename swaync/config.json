{
    "$schema": "/etc/xdg/swaync/configSchema.json",
    "positionY": "top",
    "cssPriority": "user",
    "control-center-margin-bottom": 40,
    "timeout": 5,
    "timeout-low": 3,
    "fit-to-screen": true,
    "control-center-width": 470,
    "control-center-height": 470,
    "notification-window-width": 470,
    "image-visibility": "always",
    "hide-on-clear": true,
    "widgets": [
        "title",
        "dnd",
        "mpris",
        "notifications"
    ],
    "widget-config": {
        "title": {
            "text": "Notifications",
            "clear-all-button": true,
            "button-text": "Clear All"
        },
        "dnd": {
            "text": "Do Not Disturb"
        },
        "label": {
            "max-lines": 5,
            "text": "Label Text"
        },
        "mpris": {
            "image-size": 96,
            "image-radius": 12
        }
    }
}
